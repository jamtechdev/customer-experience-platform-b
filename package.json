{
  "name": "customer-experience-platform-backend",
  "version": "1.0.0",
  "description": "Backend API for Customer Experience Measuring Platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "migrate": "sequelize-cli db:migrate",
