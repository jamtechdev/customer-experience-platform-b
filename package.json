    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/pdfkit": "^0.17.5",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
