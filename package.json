    "seed:undo": "sequelize-cli db:seed:undo"
  },
  "keywords": [
    "customer-experience",
    "analytics",
    "sentiment-analysis"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.18.0",
    "openai": "^4.52.7",
    "pdfkit": "^0.17.2",
